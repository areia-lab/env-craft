{
    "name": "areia-lab/env-craft",
    "description": "Simple UI + Artisan manager for .env with backup/restore",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "env",
        ".env",
        "environment",
        "configuration",
        "admin",
        "ui",
        "backup",
        "restore",
        "artisan",
        "env-manager",
        "env-craft"
    ],
    "authors": [
        {
            "name": "Areia Lab",
            "email": "support@areiatech.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "AreiaLab\\EnvCraft\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AreiaLab\\EnvCraft\\EnvCraftServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}